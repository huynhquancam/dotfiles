alias ll="ls -la"
alias vim='/usr/local/opt/vim/bin/vim'
alias ga='git add .'
alias gb='git branch'
alias gbd='git branch -D $1'
alias gcm='git commit'
alias gst='git status'
alias gco='git checkout'
alias gp='git push'
alias gpl='git pull'
alias gfn='git finish'
alias bi='bundle install --path .bundle/'
alias bu='bundle update --path .bundle/'
alias be='bundle exec --path .bundle'
alias docker-clean='docker rmi $(docker ps -aq --filter dangling=true)'
alias gem-ctags='ctags -R . $(bundle list --paths)'
alias cp-ssh-key='cat ~/.ssh/id_rsa.pub | pbcopy'
